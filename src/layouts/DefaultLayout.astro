---
interface Props {
  title: string;
}

const { title } = Astro.props;
---
<html>
    <head>
        <title>{title}</title>
        <link rel="icon" type="image/svg+xml" href="/img/helmet.svg" />
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
    </head>
    <body>
        <slot />
    </body>
</html>